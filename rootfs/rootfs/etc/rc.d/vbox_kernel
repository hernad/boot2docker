#!/bin/sh

. /etc/green_common

if ( is_vbox ) || ( ! scaleway_virtual_server ) || ( ! vultr_server )
then
 log_msg "virtual server vbox, scaleway, vultr - NO virtualbox host server"
 exit 0
fi

log_msg "Loading virtualbox host server kernel drivers"
modprobe vboxdrv
modprobe vboxnetadp
modprobe vboxnetflt
modprobe vboxpci
