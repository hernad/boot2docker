DISPLAY f1
PROMPT 1
TIMEOUT 50
TOTALTIMEOUT 3000

default {{ISO_DEFAULT}}
LABEL rack
  MENU LABEL greenbox ttyS0 console
	KERNEL /boot/vmlinuz64
  APPEND initrd=/boot/initrd.img {{ISO_APPEND}} console=ttyS0,38400n8 loglevel=7

LABEL vbox
  MENU LABEL greenbox vga console
	KERNEL /boot/vmlinuz64
	APPEND initrd=/boot/initrd.img {{ISO_APPEND}} loglevel=7

# help
F1 f1
F2 f2
F3 f3
F4 f4
